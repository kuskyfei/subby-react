    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "standard": {
    "ignore": [
      "build/*"
    ],
    "parser": "babel-eslint"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/media.js",
